<?php
/**
 * .env.example - 环境变量模板
 * 
 * 该文件提供了环境变量的模板，用于配置API密钥和URL。
 * 使用时，请复制此文件为.env并填入实际的API密钥和URL。
 */

# 搜索API配置
SEARCH_PROVIDER=exa
SEARCH_API_KEY=your-exa-api-key
SEARCH_API_URL=https://api.exa.ai/search

# 分析API配置 - DashScope
ANALYSIS_DASHSCOPE_API_KEY=your-dashscope-api-key
ANALYSIS_DASHSCOPE_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
ANALYSIS_DASHSCOPE_MODEL=qwen-plus-latest

# 分析API配置 - OpenAI
ANALYSIS_OPENAI_API_KEY=your-openai-api-key
ANALYSIS_OPENAI_API_URL=https://api.openai.com/v1/chat/completions
ANALYSIS_OPENAI_MODEL=gpt-4o

# 日志配置
LOGGING_ENABLED=true
LOGGING_FILE=logs/deep_research.log

# 深度研究配置
RESEARCH_DEFAULT_DEPTH=3
RESEARCH_MAX_DEPTH=10
